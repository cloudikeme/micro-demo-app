    "shell": {
      "init_hook": [
        "alias cat='bat --paging never --theme DarkNeon --style plain'",
        "alias ls='eza --long --color always --all --no-permissions --no-user --no-user --no-time --no-filesize --git'",
        "alias p='cd ~/dev/github'",
        "alias b='cd ..'",
        "alias k='kubectl'",
        "alias a='git add .'",
        "alias s='git commit -m \"$@\"'",
        "alias d='git push -u origin main'",
        "alias e='git push -u upstream main'",
        "alias q='git push -u origin dev'",
        "alias devbox='cd ~/devbox && devbox shell'",
        "alias blog='cd ~/blog.cloudikeme.com && code .'"
